# imgflo-web environment variables
# Copy this file to .env and fill in the values

# Development settings
NODE_ENV=development

# Sandbox mode (enables ccdsp alias for Claude Code without permission prompts)
# Only enable this in isolated/sandboxed environments
IS_SANDBOX=0

# Optional: Override default ports
# IMGFLO_WEB_PORT=5174
# IMGFLO_WEB_CHROME_PORT=9224

# Optional: Custom container prefix (useful when running multiple instances)
# CONTAINER_PREFIX=imgflo-web
